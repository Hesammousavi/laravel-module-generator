{
    "name": "dcblogdev/laravel-generator",
    "description": "Generate boilerplate code for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@dcblog.dev",
            "homepage": "https://dcblog.dev"
        }
    ],
    "homepage": "https://github.com/dcblogdev/laravel-generator",
    "keywords": ["Laravel", "Generator"],
    "require": {
        "symfony/filesystem": "^5.0|^6.0",
        "symfony/finder": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dcblogdev\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcblogdev\\Generator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcblogdev\\Generator\\GeneratorServiceProvider"
            ],
            "aliases": {
                "Generator": "Dcblogdev\\Generator\\Facades\\Generator"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.1"
    }
}
